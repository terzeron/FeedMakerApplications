        <list_url_list>
            <list_url><![CDATA[http://mmzone.co.kr/album/showcase_section.php?dbname=smz_main&section=2]]></list_url>
        </list_url_list>
        <element_list>
            <element_id>scm_left_block</element_id>
        </element_list>
        <item_capture_script>./capture_item_link_title.py -n 10</item_capture_script>
        <post_process_script_list>
            <!--post_process_script>grep -v '[질답]'</post_process_script-->
        </post_process_script_list>
    </collection>
